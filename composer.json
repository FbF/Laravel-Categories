{
    "name": "fbf/laravel-categories",
    "description": "A Laravel 4 package for adding one or more types of category hierarchies to a website",
    "keywords": ["laravel","categories","hierarchy"],
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "baum/baum": "~1.0",
        "cviebrock/eloquent-sluggable": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "Fbf\\LaravelCategories\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
